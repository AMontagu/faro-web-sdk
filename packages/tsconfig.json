{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@grafana/agent-*": ["*/src"],
      "#core/*": ["core/src/*"],
      "#demo/*": ["demo/src/*"],
      "#web/*": ["web/src/*"],
      "#web-tracing/*": ["web-tracing/src/*"],
      "#integration-angular/*": ["integration-angular/src/*"],
      "#integration-react/*": ["integration-react/src/*"],
      "#*": ["*/src"]
    },
    "rootDir": "."
  }
}
